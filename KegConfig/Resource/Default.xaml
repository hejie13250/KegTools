<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <FontFamily x:Key="FontFamilyResource">微软雅黑</FontFamily>

  <Style TargetType="{x:Type TextBlock}">
    <Setter Property="FontFamily" Value="{DynamicResource FontFamilyResource}" />
  </Style>
  <Style TargetType="{x:Type TextBox}">
    <Setter Property="FontFamily" Value="{DynamicResource FontFamilyResource}" />
  </Style>
  <Style TargetType="{x:Type Label}">
    <Setter Property="FontFamily" Value="{DynamicResource FontFamilyResource}" />
  </Style>

  <ImageBrush x:Key="DefaultBrush" ImageSource="/background/默认.jpg" />

  <Style x:Key="AlphaSliderStyle" TargetType="{x:Type Slider}">
    <Setter Property="Orientation" Value="Vertical" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Slider}">
          <Grid>
            <Border
              x:Name="TrackBackground"
              Width="20"
              HorizontalAlignment="Center"
              BorderBrush="Gray"
              BorderThickness="1">
              <Border.Background>
                <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                  <GradientStop Color="#ffFFFFFF" />
                  <GradientStop Offset="1" Color="#00FFFFFF" />
                </LinearGradientBrush>
              </Border.Background>
            </Border>
            <Track x:Name="PART_Track">
              <Track.Thumb>
                <Thumb>
                  <Thumb.Template>
                    <ControlTemplate TargetType="{x:Type Thumb}">
                      <Ellipse
                        Width="12"
                        Height="6"
                        Fill="Red" />
                    </ControlTemplate>
                  </Thumb.Template>
                </Thumb>
              </Track.Thumb>
            </Track>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>


  <Style x:Key="ColorAlphaSliderStyle" TargetType="{x:Type Slider}">
    <Setter Property="Orientation" Value="Horizontal" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Slider}">
          <Grid>
            <Border
              x:Name="TrackBackground"
              Height="20"
              VerticalAlignment="Center"
              BorderBrush="Gray"
              BorderThickness="1">
              <Border.Background>
                <LinearGradientBrush StartPoint="0,0" EndPoint="1,0">
                  <GradientStop Color="#00FFFFFF" />
                  <GradientStop Offset="1" Color="#FFFFFFFF" />
                </LinearGradientBrush>
              </Border.Background>
            </Border>
            <Track x:Name="PART_Track">
              <Track.Thumb>
                <Thumb>
                  <Thumb.Template>
                    <ControlTemplate TargetType="{x:Type Thumb}">
                      <Ellipse
                        Width="6"
                        Height="12"
                        Fill="Red" />
                    </ControlTemplate>
                  </Thumb.Template>
                </Thumb>
              </Track.Thumb>
            </Track>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

